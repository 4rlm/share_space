
<% if @address.present? && @address.full_address%>
  <iframe width="500" height="500" frameborder="1" style="border:1"
   src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCMINjz6qATKxuxTc6tWNzyVehLybEYouY&zoom=17&q=<%=@address.full_address %>"
   allowfullscreen>
  </iframe>
<% else %>
<div class="row text-center">
  <h5><em>No Address for this Space</em></h5>
</div>
<% end %>
