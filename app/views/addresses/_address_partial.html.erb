
<div class="row">

  <div class="col-md-6">
    <%#= binding.pry %>

    <p>
      <strong>Space:</strong>
      <%= @address.space_id %>
    </p>

    <p>
      <strong>Street:</strong>
      <%= @address.street %>
    </p>

    <p>
      <strong>Unit:</strong>
      <%= @address.unit %>
    </p>

    <p>
      <strong>City:</strong>
      <%= @address.city %>
    </p>

    <p>
      <strong>State:</strong>
      <%= @address.state %>
    </p>

    <p>
      <strong>Zip:</strong>
      <%= @address.zip %>
    </p>

    <p>
      <strong>Country:</strong>
      <%= @address.country %>
    </p>

  </div>

  <div class="col-md-6">
    <div class="row">
      <%= render 'addresses/embeded_map_partial', address: @address %>
    </div>
  </div>

</div>


<%= link_to 'Edit', edit_address_path(@address) %>
 <%if !@space %>
 | <%= link_to 'Back', addresses_path %>
 <%end%>
